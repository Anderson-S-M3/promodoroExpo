{
  "expo": {
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "visible": "sticky-immersive"
    },
    "name": "Promodoro",
    "slug": "Promodoro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anderson.promodoro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anderson.promodoro"
    }
  }
}
